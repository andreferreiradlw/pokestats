  },
  "homepage": "https://pokestats.gg/",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src -c .eslintrc --ext js",
    "prettier": "prettier \"**/*.{js,ts,md,json,yml}\" --ignore-path .gitignore",
