{
  "name": "pokestats",
  "version": "0.2.0",
  "description": "PokeStats.gg is an online encyclopedia of Pokémon species containing information such as Pokédex entries, descriptions, evolution chains, moves, stats and much more.",
  "keywords": [
    "pokemon",
    "pokedex",
    "pokestats",
    "pokeapi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andreferreiradlw/pokestats.git"
  },
