    "@tanstack/react-query": "^5.59.0",
    "@tsparticles/engine": "^3.5.0",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.5.0",
    "formik": "^2.4.6",
    "framer-motion": "^11.11.1",
    "next": "14.2.14",
    "pokenode-ts": "^1.20.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "sharp": "^0.33.5"
